{
  "name": "vui-vc-next",
  "version": "0.13.8",
  "description": "Mobile web UI components based on Vue 3 and Vite",
  "files": [
    "dist/style.css",
    "dist/vui-vc-next.es.js",
    "dist/vui-vc-next.umd.js"
  ],
  "main": "dist/vui-vc-next.umd.js",
  "module": "dist/vui-vc-next.es.js",
  "exports": {
    ".": {
      "import": "./dist/vui-vc-next.es.js",
      "require": "./dist/vui-vc-next.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluepower/vui-vc-next.git"
  },
  "keywords": [
    "vue",
    "vue-components",
    "vue3",
    "vue3-components",
    "vite",
    "vui",
    "vue-ui",
    "vue3-ui",
    "vue3-vite",
    "vue3-vite-ui",
    "vue-composition-api",
    "vitepress",
    "vue3-vite-vitepress",
    "vue3-vite-vitepress-ui",
    "vite2",
    "vue3-vite2-ui",
    "vue3-vite2-vitepress-ui"
  ],
  "author": "Niko Ni",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bluepower/vui-vc-next/issues"
  },
  "homepage": "https://github.com/bluepower/vui-vc-next/blob/dev/README.md",
  "scripts": {
    "dev": "vite",
    "dev-docs": "cd docs && vitepress",
    "build": "vite build --config lib/vite.config.js",
    "build-demo": "vite build",
    "build-docs": "cd docs && vitepress build",
    "serve": "vite preview",
    "serve-build": "vite build && cd ./dist && vite"
  },
  "dependencies": {
    "vue": "^3.0.10"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.2.1",
    "@vue/compiler-sfc": "^3.0.10",
    "postcss-pxtorem": "^6.0.0",
    "sass": "^1.32.8",
    "vite": "^2.1.5",
    "vitepress": "^0.12.2",
    "vue-router": "^4.0.5"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "browserslist": [
    "iOS >= 8",
    "Android > 4.0"
  ]
}
